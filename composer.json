{
    "name": "thedevsbuddy/adminr-core",
    "version": "0.1.1",
    "description": "A simple yet powerful Laravel project starter with admin panel and CRUD Resources generator.",
    "keywords": [
        "devsbuddy",
        "adminr-core"
    ],
    "homepage": "https://github.com/thedevsbuddy/adminr-core",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Shoaib Khan",
            "email": "shoaibkhan@devsbuddy.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Devsbuddy\\AdminrCore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devsbuddy\\AdminrCore\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devsbuddy\\AdminrCore\\AdminrCoreServiceProvider"
            ],
            "aliases": {
                "AdminrCore": "Devsbuddy\\AdminrCore\\AdminrCoreFacade"
            }
        }
    }
}
